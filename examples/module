int inum, num

function write() {
	if(num == 0) {
		print("zero")
	} else if(num == 1) {
		print("one")
	} else if(num == 2) {
		print("two")
	} else if(num == 3) {
		print("three")
	} else if(num == 4) {
		print("four")
	} else if(num == 5) {
		print("five")
	} else if(num == 6) {
		print("six")
	} else if(num == 7) {
		print("seven")
	} else if(num == 8) {
		print("eigth")
	} else {
		print("nine")
	}
}

input(inum)

while(inum > 0) {
	num = inum % 10
	write()
	inum = inum / 10
}